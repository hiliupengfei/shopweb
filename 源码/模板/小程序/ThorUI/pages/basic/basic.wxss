.header {
  padding: 80rpx 60rpx;
}

.title {
  font-size: 36rpx;
  color: #333;
  font-weight: bold;
}

.sub-title {
  font-size: 28rpx;
  color: #7a7a7a;
  padding-top: 18rpx;
}

.tui-grid-icon {
  width: 74rpx;
  height: 74rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tui-grid-icon image {
  display: block;
}

.tui-grid-icon + .tui-grid-label {
  margin-top: 10rpx;
}

.tui-grid-label {
  display: block;
  text-align: center;
  font-weight: 400;
  color: #333;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
