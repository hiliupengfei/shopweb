<view class="container">
  <view class="list-item">省份：{{obj.liveData.province}}</view>
  <view class="list-item">位置：{{obj.liveData.city}}</view>
  <view class="list-item">湿度：{{obj.liveData.humidity}}%</view>
  <view class="list-item">温度：{{obj.liveData.temperature}} 摄氏度</view>
  <view class="list-item">天气：{{obj.liveData.weather}}</view>
  <view class="list-item">风向：{{obj.liveData.winddirection}}</view>
  <view class="list-item">风力：{{obj.liveData.windpower}}级</view>
   <view class="btn-box">
    <button class="btn-primary" hover-class="btn-hover" bindtap="back">返回</button>
  </view>

</view>
